# Netnews generic makefile to be customized for your site by Configure

SHELL = /bin/sh

#
# Here are the macro definitions that Configure tweaks
#
NEWSUSR = news
NEWSGRP = news
TEXTDIR = /usr/spool/news/
SPOOLDIR = /usr/spool/news/.rnews
BATCHDIR = /usr/spool/batch
LIBDIR = /usr/lib/news
ADMDIR = /usr/lib/news
BINDIR = /usr/local/bin
UUBINDIR = /usr/bin
#NONLOCAL NNTPSRC = /usr/src/nntp
#NONLOCAL NNTPDIR = /usr/src/nntp/common
#POSIXCOMPAT POSIXLIB = libposix.a
LNRNEWS = ln
SIZEDEFS =
CC = cc
LD = cc
CHOWN=chown
CHGRP=chgrp
TERMLIB =
SCRIPTS =
LOCLIBS =
NETLIB =
SPECIES =
GENUS =
MAKE=make
CP=cp

#
# The following only change if the shape of the news source hierarchy does
#
MAINDIR = .
PORTDIR = D.port
NEWSDIR = D.news
READDIR = D.read
FILTDIR = D.filt
POSTDIR = D.post
SCRNDIR = D.scrn
PRIVDIR = D.priv
UUCPDIR = D.uucp
NNETDIR = D.network
#POSIXCOMPAT POSXDIR = D.posix

#LOCAL LSOURCES = $(PRIVDIR) $(UUCPDIR)
#NETWORK LSOURCES = $(NNETDIR)
#LOCAL LHEADERS = -I$(PRIVDIR) -I$(UUCPDIR)
#NETWORK LHEADERS = -I$(NNETDIR)

DIRLIST = $(POSXDIR) $(PORTDIR) $(NEWSDIR) $(READDIR) $(FILTDIR) $(POSTDIR) $(SCRNDIR) \
	$(LSOURCES) 
INCLUDE = -I$(PORTDIR) -I$(NEWSDIR) -I$(READDIR) -I$(FILTDIR) -I$(POSTDIR) \
	-I$(SCRNDIR) $(LHEADERS) $(NNTPDIR)
SCRIPTDIR = D.scripts

#
# System-specific compiler and linker flags are merged in here
#
CSPECIAL =
LSPECIAL =
#CDEBUG = -g
CDEBUG = -O
CFLAGS = -I. -ID.include ${CDEBUG} ${CSPECIAL} $(CF)
#LDEBUG = -lg
LDEBUG = -s
LFLAGS = ${LDEBUG} ${LSPECIAL}
LIBS =

#V7B LINTFLAGS = -chba -I. -ID.include
#USG LINTFLAGS = -I. -ID.include

#BSD4_1 LIBS = -ljobs
#UNIXPC LIBS = -lposix	#...because there's no room left in shlib, @#$@#!!% it

#
# If you'll never need some of the news tools, here's where to comment them out
#
READERS = vnews checknews edvnews mailnews readnews ednews vrn
FILTERS = rnkill
#LOOSE USRCMDS = $(READERS) $(FILTERS) postnews bbsauto
#TIGHT USRCMDS = $(READERS) $(FILTERS) postnews bbsauto locknews
#LOCAL LIBCMDS = rnews expire sendbatch inews uurec caesar
AUXCMDS = compress

# Everything between the next line and the matching line below is sacred
#----------------------------------------------------------------------
#
COMMANDS = $(LOCLIBS) $(USRCMDS) $(LIBCMDS) $(AUXCMDS)
TESTERS = newsdb ednewsipc
HELPFILES = readnews.help vnews.help vrn.group.help vrn.art.help vrn.pager.help

# Use this default to make updated commands
all: makefiles libraries $(COMMANDS) makekits
	@echo "Done at "`date`

profiling: profiled/expire profiled/rnews

.PHONY: all makefiles libraries quietly profiling

#PROFLIB .c.o:
#PROFLIB 	${CC} -p ${CFLAGS} -c $*.c
#PROFLIB 	mv $*.o profiled/$*.o
#PROFLIB 	${CC} ${CFLAGS} -c $*.c

saber_cflags:
	#setopt load_flags $(CFLAGS)

makefiles:
	-$(MAKE) -$(MAKEFLAGS) Makefile;
	for x in $(DIRLIST); do (cd $$x; echo "$$x:"; $(MAKE) -$(MAKEFLAGS) -f Makefile.dst Makefile); done

Makefile: Makefile.dst confignews.sh
	$(MAINDIR)/GenerateMake $(DIRLIST)

# On the first make, generate Makefile once just to set $(DIRLIST) properly,
# then force a re-generation so GenerateMake will attach correct prefixes
firstmake: confignews.sh configsys.sh Makefile.dst
	./GenerateMake
	sleep 2; touch confignews.sh
	$(MAKE) -$(MAKEFLAGS) makefiles D.include
	-mkdir profiled

# This kluge is required by BSD's hardcoded limit of at most 8 -Is per cc
D.include:
	mkdir D.include
	mkdir D.include/sys
	ln D.*/*.h D.include
	ln D.posix/sys.dirent.h D.include/sys/dirent.h

libraries:
	for x in $(DIRLIST); do (cd $$x; echo "$$x:"; $(MAKE) -$(MAKEFLAGS) "CDEBUG=$(CDEBUG)" export); done

quietly:
	(nohup $(MAKE) -$(MAKEFLAGS) -k) >make.log 2>&1 &

# Use this to install the system
#LOCAL installnews: usrcmds transport
#NETWORK installnews: usrcmds
	sh ./InstallNews

usrcmds: bindir $(HELPFILES)
transport: libdir uubindir scripts $(ADMDIR) $(TEXTDIR) $(BATCHDIR) $(SPOOLDIR)

bindir: $(BINDIR)
#V7B	for x in $(USRCMDS); \
#V7B		do install -o $(NEWSUSR) -g $(NEWSGRP) $$x $(BINDIR); done
#!V7B	$(CP) $(USRCMDS) $(BINDIR);
	-cd $(BINDIR); \
#!V7B		chown $(NEWSUSR) $(USRCMDS); \
#!V7B		chgrp $(NEWSGRP) $(USRCMDS); \
#LOOSE		chmod u=rwx,go=rx $(USRCMDS)
#TIGHT		chmod u=rwx,go=rx,g+s $(USRCMDS); chmod u+s locknews

libdir: $(LIBDIR)
#V7B	for x in $(LIBCMDS) $(AUXCMDS); \
#V7B		do install -o $(NEWSUSR) -g $(NEWSGRP) $$x $(LIBDIR); done
#!V7B	$(CP) $(LIBCMDS) $(AUXCMDS) $(LIBDIR)
	cd $(LIBDIR);\
#!V7B		chown $(NEWSUSR) $(LIBCMDS); \
#!V7B		chgrp $(NEWSGRP) $(LIBCMDS); \
		chmod u=rwx,go=rx $(LIBCMDS) $(AUXCMDS);\
#NEWCTRL	chmod u=rwx,go=r control;\
		chmod u+s,g+s expire rnews sendbatch;
	cp $(HELPFILES) $(LIBDIR)

uubindir:
	-cd $(UUBINDIR); rm -f rnews unbatch cunbatch c7unbatch
	${LNRNEWS} $(LIBDIR)/rnews $(UUBINDIR)/rnews
	${LNRNEWS} $(LIBDIR)/rnews $(UUBINDIR)/unbatch
	${LNRNEWS} $(LIBDIR)/rnews $(UUBINDIR)/cunbatch
	${LNRNEWS} $(LIBDIR)/rnews $(UUBINDIR)/c7unbatch

MAPCONFIG = ConfigureMap ConfigureUtils configsys.sh confignews.sh
scripts:
	-rm -f $(SCRIPTDIR)/config.sed
	-ln config.sed $(SCRIPTDIR)
	cd $(SCRIPTDIR); sh config.sed $(SCRIPTS)
	cp $(MAPCONFIG) $(LIBDIR)
	cd $(LIBDIR);\
		$(CHOWN) $(NEWSUSR) $(SCRIPTS);\
		$(CHGRP) $(NEWSGRP) $(SCRIPTS);\
		chmod u=rwx,go=rx $(SCRIPTS) $(MAPCONFIG)

$(BINDIR)::
	-mkdir $(BINDIR)
	$(CHOWN) $(NEWSUSR) $(BINDIR)
	$(CHGRP) $(NEWSGRP) $(BINDIR)
	chmod u=rwx,go=rx $(BINDIR)

$(BATCHDIR)::
	-mkdir $(BATCHDIR)
	$(CHOWN) $(NEWSUSR) $(BATCHDIR)
	$(CHGRP) $(NEWSGRP) $(BATCHDIR)
	chmod u=rwx,go=rx $(BATCHDIR)

$(LIBDIR)::
	-mkdir $(LIBDIR)
	$(CHOWN) $(NEWSUSR) $(LIBDIR)
	$(CHGRP) $(NEWSGRP) $(LIBDIR)
	chmod u=rwx,go=rx $(LIBDIR)

$(ADMDIR)::
	-mkdir $(ADMDIR)
	$(CHOWN) $(NEWSUSR) $(ADMDIR)
	$(CHGRP) $(NEWSGRP) $(ADMDIR)
	chmod u=rwx,go=rx $(ADMDIR)

$(TEXTDIR)::
	-mkdir $(TEXTDIR) $(TEXTDIR)/.tmp $(TEXTDIR)/.bad
	$(CHOWN) $(NEWSUSR) $(TEXTDIR) $(TEXTDIR)/.tmp $(TEXTDIR)/.bad
	$(CHGRP) $(NEWSGRP) $(TEXTDIR) $(TEXTDIR)/.tmp $(TEXTDIR)/.bad
	chmod u=rwx,go=rx $(TEXTDIR) $(TEXTDIR)/.tmp $(TEXTDIR)/.bad

$(SPOOLDIR)::
	-mkdir $(SPOOLDIR)
	$(CHOWN) $(NEWSUSR) $(SPOOLDIR)
	$(CHGRP) $(NEWSGRP) $(SPOOLDIR)
	chmod u=rwx,go=rx $(SPOOLDIR)

# This is strictly for TIGHT-testing purposes
usrperms:
	$(CHGRP) $(NEWSGRP) $(USRCMDS);
#LOOSE	chmod u=rwx,go=rx $(USRCMDS)
#TIGHT	chmod u=rwx,go=rx,g+s $(USRCMDS); chmod u+s locknews

# Install a fake rnews for debugging purposes (you must be su news or root)
fake: $(BATCHDIR)
	rm -f $(UUBINDIR)/rnews
	echo "cat >$(BATCHDIR)/inbatch\$$\$$" >$(UUBINDIR)/rnews
	chmod +x $(UUBINDIR)/rnews

# Install a fake inews for use during conversions (you must be su news or root)
SORRY="Posting is temporarily disabled while a new version is being installed"
ifake:
	echo "echo \"" $(SORRY) "\"" >$(LIBDIR)/inews
	chmod +x $(LIBDIR)/inews

# Prepare to package a distribution
distclean:
	rm -f grpw $(USRCMDS) $(LIBCMDS) $(AUXCMDS) makekits $(TESTERS)
	for x in $(DIRLIST); do (cd $$x; echo "$$x:"; $(MAKE) -$(MAKEFLAGS) \
		-f Makefile.dst distclean); done

# Throw away all object and generated files
clean: distclean
	for x in $(DIRLIST); do (cd $$x; echo "$$x:"; $(MAKE) -$(MAKEFLAGS) \
		-f Makefile.dst clean); done
	rm -f core a.out *.log *.ln llib-* *~ shlib.ifile TAGS *.[oa]
	rm -fr D.include profiled

# This production clears the USENET history data bases and article tree
reset:
	cd $(LIBDIR); rm -f log errlog old* history* LOCK EXPLOCK
	cd $(TEXTDIR); rm -fr *
	cd $(BATCHDIR); rm -fr *
	sh ./InstallNews

# This production clears all the USENET data bases and the article tree
purge:
	cd $(LIBDIR); rm -f log errlog old* history* LOCK EXPLOCK active
	cd $(TEXTDIR); rm -fr *
	cd $(BATCHDIR); rm -fr *
	sh ./InstallNews

# This production clobbers everything
clobber:
	cd $(LIBDIR); rm -fr *
	cd $(BATCHDIR); rm -fr *
	cd $(TEXTDIR); rm -fr *
	cd $(SPOOLDIR); rm -fr *

#
