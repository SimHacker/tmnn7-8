# Don't mess with the link order on this one, XENIX ld gets upset
locknews: locknews.o libpriv.a libnews.a libport.a
	$(LD) $(LFLAGS) locknews.o libnews.a libport.a libpriv.a $(LIBS) -o locknews

ELIBS = libpriv.a libnews.a libport.a
expire: expire.o $(ELIBS)
	$(LD) $(LFLAGS) expire.o $(ELIBS) $(LIBS) -o expire

expire.o: expire.c
	$(CC) $(CFLAGS) -c $*.c

profiled/expire.o: expire.c
	@rm -f $*_p.c
	ln $*.c $*_p.c
	$(CC) -pg -DPROFILE $(CFLAGS) -c $*_p.c
	mv $*_p.o profiled/$*.o

profiled/expire: profiled/expire.o $(ELIBS)
	(cd profiled; $(LD) $(LFLAGS) expire.o $(ELIBS) $(LIBS) -pg -o expire)

