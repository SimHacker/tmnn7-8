.SUFFIXES: .ln

.c.ln:
#LINTLIBS	lint $(LINTFLAGS) -c $<
#NOLINTLIBS	lint $(LINTFLAGS) -C$* $<
#NOLINTLIBS	mv llib-l$*.ln $*.ln

# LINT AUTO-DEPENDENCIES GO AFTER THIS LINE -- DO NOT DELETE IT!!!

#CURSES SYSLINT=-lcurses

lintlibs:
	for x in $(DIRLIST); do (cd $$x; echo "$$x:"; $(MAKE) -$(MAKEFLAGS) lintlib); done

ALLRDLNT= llib-lread.ln llib-lfilt.ln llib-lpost.ln llib-lnews.ln llib-lport.ln
rlint: $(ALLRDLNT) checknews.ln mailnews.ln ednews.ln readnews.ln vnews.ln
	lint $(LINTFLAGS) checknews.ln $(ALLRDLNT)
	lint $(LINTFLAGS) mailnews.ln $(ALLRDLNT)
	lint $(LINTFLAGS) ednews.ln $(ALLRDLNT)
	lint $(LINTFLAGS) readnews.ln $(ALLRDLNT)

flint: $(ALLRDLNT) rnkill.ln
	lint $(LINTFLAGS) rnkill.ln $(ALLRDLNT)

vlint: vnews.ln vrn.ln llib-lscrn.ln $(ALLRDLNT)
	lint $(LINTFLAGS) vnews.ln llib-lscrn.ln $(ALLRDLNT) $(SYSLINT)
	lint $(LINTFLAGS) vrn.ln llib-lscrn.ln $(ALLRDLNT) $(SYSLINT)

plint: postnews.ln inews.ln llib-lpost.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) postnews.ln llib-lpost.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) inews.ln llib-lpost.ln llib-lnews.ln llib-lport.ln

elint: expire.ln llib-lpriv.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) expire.ln llib-lpriv.ln llib-lnews.ln llib-lport.ln

slint: sendbatch.ln llib-luucp.ln llib-lpriv.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) sendbatch.ln llib-luucp.ln llib-lpriv.ln llib-lnews.ln llib-lport.ln

ulint: uurec.ln locknews.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) uurec.ln llib-lnews.ln llib-lport.ln
	lint $(LINTFLAGS) locknews.ln llib-lnews.ln llib-lport.ln

RSRCS = rnews.c unbatch.c insert.c post.c dispatch.c control.c
RWLINTS = rnews.ln unbatch.ln insert.ln post.ln dispatch.ln control.ln
RCLLIBS = llib-luucp.ln llib-lpriv.ln llib-lnews.ln llib-lport.ln
nlint: $(RWLINTS) $(RCLLIBS)
	lint $(LINTFLAGS) $(RWLINTS) $(RCLLIBS)

OSRCS = caesar.c compress.c grpw.c makekits.c
olint:	
	lint $(LINTFLAGS) -lm caesar.c
	lint $(LINTFLAGS) -D$(GENUS) compress.c
	lint $(LINTFLAGS) grpw.c
	lint $(LINTFLAGS) makekits.c

lintall: makefiles lintlibs rlint flint vlint plint nlint elint slint ulint olint
	@echo "Done at "`date`

lint:	
	exec make lintall >lint.log 2>&1 &

#
